{
    "name": "termyn/symfony-bundle",
    "description": "Integration of the `termyn` toolkit into Symfony 4.3<",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edo Barnas",
            "email": "iam@edobarnas.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Termyn\\Symfony\\Bundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Termyn\\Symfony\\Bundle\\Test\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "termyn/cqrs": "0.3.*",
        "termyn/date-time": "0.1.*",
        "termyn/ddd": "0.2.*",
        "termyn/id": "0.1.*",
        "termyn/event": "0.1.*",
        "termyn/money": "dev-main",
        "symfony/framework-bundle": "^4.3|5.*|6.*|7.*"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^10.5",
        "symplify/easy-coding-standard": "^12.1"
    }
}
